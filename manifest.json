{
  "manifest_version":2,
  "name": "Add okcamp",
  "version": "0.2",
  "description": "add gear from amazon to okcamp",
  "permissions": [
    "tabs",
    "background",
    "http://localhost:3000/",
    "http://www.okcamp.me/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.amazon.co.jp/*",
        "https://www.amazon.co.jp/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "background":{
    "scripts":[
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
