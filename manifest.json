{
  "manifest_version":2,
  "name": "Add gear",
  "version": "1.0.2",
  "description": "Add gear data to okcamp dev tool",
  "permissions": [
    "tabs",
    "background",
    "http://localhost:3000/",
    "https://okcamp.me/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.amazon.co.jp/*",
        "https://www.amazon.co.jp/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "background":{
    "scripts":[
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
